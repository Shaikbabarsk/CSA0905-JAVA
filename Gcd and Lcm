import java.util.*;

class GcdLcm

{

 public static void main(String args[])

{

 Scanner s=new Scanner(System.in)

 int gcd=0,lcm,n1,n2;

 System.out.println("Enter n1 value");

 n1=s.nextInt();

 System.out.println("Enter n2 value");

 n2=s.nextInt()

 for(i=1;i<=n1 && i<=n2;i++)

 {

  if(n1%i==0 && n2%i==0)

  {

    gcd=i;

  }

}
System.out.println(gcd);

lcm=(n1*n2)/gcd;

System.out.println(lcm);

}

}
